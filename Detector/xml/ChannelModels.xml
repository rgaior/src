<?xml version="1.0"?>
<ChannelModels>

   <!-- low Ku band setup: without Soontai bpf and MiniCircuit hpf -->
   <channel type="lowKu1">
      <!-- number of trace bins -->
      <nTraceBins> 25000 </nTraceBins>
      <!-- start time of trace in sec -->
      <traceStartTime> -10e-6 </traceStartTime>
      <!-- band width in Hz -->
      <bandWidth> 1.0e9 </bandWidth>
      <!-- center frequency in Hz -->
      <centerFrequency> 11.2e9 </centerFrequency>
      <noise> 50 </noise>
      <!-- time width of trace bin in sec -->
      <traceBinWidth> 0.8e-9 </traceBinWidth>
   </channel>

